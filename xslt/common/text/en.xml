<?xml version="1.0" encoding="utf-8" ?>
<!-- vim: sta et sw=2
-->

<l:locale lang="en" 
  xmlns:l="http://docbook2x.sourceforge.net/xsl/localization"
  xmlns:dl="http://docbook2x.sourceforge.net/xsl/document-localization"
  >

  <l:locale-info>
    <l:english-language-name>English</l:english-language-name>
    <l:native-language-name>English</l:native-language-name>
  </l:locale-info>
         
  <!-- Admonitions -->
  <dl:title element="caution">Caution</dl:title>
  <dl:title element="important">Important</dl:title>
  <dl:title element="note">Note</dl:title>
  <dl:title element="tip">Tip</dl:title>
  <dl:title element="warning">Warning</dl:title>

  <!-- Divisions -->
  <dl:title element="article">Article</dl:title>
  <dl:title element="book">Book</dl:title>
  <dl:title element="part">Part</dl:title>
  <dl:title element="set">Set</dl:title>

  <!-- Components -->
  <dl:title element="abstract">Abstract</dl:title>
  <dl:title element="appendix">Appendix</dl:title>
  <dl:title element="bibliography">Bibliography</dl:title>
  <dl:title element="chapter">Chapter</dl:title>
  <dl:title element="colophon">Colophon</dl:title>
  <dl:title element="dedication">Dedication</dl:title>
  <dl:title element="glossary">Glossary</dl:title>
  <dl:title element="preface">Preface</dl:title>
  <dl:title element="reference">Reference</dl:title>
  <dl:title element="sidebar">Sidebar</dl:title>

  <!-- Sections -->
  <dl:title element="sect1">Section</dl:title>
  <dl:title element="sect2">Section</dl:title>
  <dl:title element="sect3">Section</dl:title>
  <dl:title element="sect4">Section</dl:title>
  <dl:title element="sect5">Section</dl:title>
  <dl:title element="section">Section</dl:title>

  <!-- Block objects -->
  <dl:title element="equation">Equation</dl:title>
  <dl:title element="example">Example</dl:title>
  <dl:title element="procedure">Procedure</dl:title>
  <dl:title element="step">step</dl:title>
  <dl:title element="table">Table</dl:title>

  <dl:title element="copyright">Copyright</dl:title>
  <dl:title element="edition">Edition</dl:title>
  <dl:title element="figure">Figure</dl:title>
  <dl:title element="index">Index</dl:title>
  <dl:title element="isbn">ISBN</dl:title>
  <dl:title element="legalnotice">Legal Notice</dl:title>
  <dl:title element="published">Published</dl:title>

  <!-- Reference entry elements -->
  <dl:title element="refentry">Reference entry</dl:title>
  <dl:title element="refnamediv">Name</dl:title>
  <dl:title element="refsection">Section</dl:title>
  <dl:title element="refsynopsisdiv">Synopsis</dl:title>
  
  <dl:title element="revhistory">Revision History</dl:title>
  <dl:title element="revision">Revision</dl:title>
  <dl:title element="setindex">Set Index</dl:title>
 
  <dl:label element="answer">A:</dl:label>
  <dl:label element="question">Q:</dl:label>
  <dl:label element="msgaud">Audience: </dl:label>
  <dl:label element="msglevel">Level: </dl:label>
  <dl:label element="msgorig">Origin: </dl:label>

  <!-- Glossary "see" rendering -->
  <dl:rendering key="glosssee">See <dl:content/></dl:rendering>
  <dl:rendering key="glossseealso">See also <dl:content/></dl:rendering>
  
  <!-- Index entry "see" rendering -->
  <dl:rendering key="see">see <dl:content/></dl:rendering>
  <dl:rendering key="seealso">see also <dl:content/></dl:rendering>

  <dl:rendering key="remark">[Remark: <dl:content/>]</dl:rendering>
  
  <!-- Error notices in the output -->
  <dl:rendering key="dangling-xref">[Cross reference to non-existant ID “<dl:content/>”]</dl:rendering>
  <dl:rendering key="element-not-matched">[Unrecognized XML element “<dl:content/>” in source document]</dl:rendering>
  <dl:rendering key="xref-non-refentry">“<dl:content/>” [not available as a man page]</dl:rendering>
  <dl:rendering key="man-link"> [<dl:content/>]</dl:rendering>
  <dl:rendering key="man-url">&#x2329;<dl:content/>&#x232A;</dl:rendering>

  <!-- Quotation marks -->
  <dl:text key="quote-start">&#x201C;</dl:text>
  <dl:text key="quote-end">&#x201D;</dl:text>
  <dl:text key="nested-quote-start">&#x2018;</dl:text>
  <dl:text key="nested-quote-end">&#x2019;</dl:text>

  <!-- Quotes that might be used for elements like literal, markup, ... -->
  <dl:text key="literal-quote-start">&#x201C;</dl:text>
  <dl:text key="literal-quote-end">&#x201D;</dl:text>
  
  <!-- List item marks -->
  <dl:text key="list-bullet">&#x2022;</dl:text>
  <dl:text key="inline-list-separator">, </dl:text>
  <dl:text key="seg-separator">: </dl:text>
  <dl:text key="run-in-title-separator">. </dl:text>

  <dl:text key="biblio-item-separator">. </dl:text>
  <dl:text key="citetitle-start-quote">&#x201C;</dl:text>
  <dl:text key="citetitle-end-quote">&#x201D;</dl:text>
  <dl:text key="year-separator">, </dl:text>

  <dl:text key="block-object-placeholder">&#xFFFC; [undisplayable block object]</dl:text>
  <dl:text key="[missing text]">[MISSING TEXT]</dl:text>

  <!-- arg/group/cmdsynopsis decorations -->
  <dl:text key="arg-choice-opt-start">[</dl:text>
  <dl:text key="arg-choice-opt-end">]</dl:text>
  <dl:text key="arg-choice-req-start">{</dl:text>
  <dl:text key="arg-choice-req-end">}</dl:text>
  <dl:text key="arg-choice-plain-start"> </dl:text>
  <dl:text key="arg-choice-plain-end"> </dl:text>
  <dl:text key="arg-choice-default-start">[</dl:text>
  <dl:text key="arg-choice-default-end">]</dl:text>
  <dl:text key="arg-rep-repeat">…</dl:text>
  <dl:text key="arg-rep-norepeat"></dl:text>

  <dl:text key="arg-or-separator"> | </dl:text>
  <dl:text key="arg-or-separator-nospace">|</dl:text>
  
  <dl:text key="cmdsynopsis-default-sepchar"> </dl:text>

  <dl:text key="Concept index">Concept index</dl:text>
  <dl:text key="Function index">Function index</dl:text>
  <dl:text key="Variables index">Variables index</dl:text>
  <dl:text key="Keystroke index">Keystroke index</dl:text>
  <dl:text key="Program index">Program index</dl:text>
  <dl:text key="Data type index">Data type index</dl:text>

  <dl:text key="uppercase-alphabet">ABCDEFGHIJKLMNOPQRSTUVWXYZ</dl:text>
  <dl:text key="lowercase-alphabet">abcdefghijklmnopqrstuvwxyz</dl:text>

  <dl:text key="--- The Detailed Node Listing ---">— The Detailed Node Listing —</dl:text>

  <!-- Stylesheet user messages -->
  <l:message key="reference to non-existent ID">reference to non-existent ID “<l:a1 />”</l:message>

</l:locale>
